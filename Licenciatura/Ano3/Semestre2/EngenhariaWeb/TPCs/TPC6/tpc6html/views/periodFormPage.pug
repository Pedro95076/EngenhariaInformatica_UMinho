extends layout 

block content
    div.w3-card-4 
        header.w3-container.w3-teal 
            h2 Period Form
            a(href="javascript:history.back()") Voltar
        
    
        form.w3-container(method="POST")
            fieldset 
                legend Metadata
                label id
                input.w3-input.w3-round(type="text" name="id") 
                label Nome
                input.w3-input.w3-round(type="text" name="nome")
            br
            button.w3-btn.w3-teal.w3-mb-2(type="submit") Register
    
        footer.w3-container.w3-teal 
            h5 Generated by A97569::EngWeb2024 in #{data}
                